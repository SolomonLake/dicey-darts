{
    "name": "dicey-darts",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW_DEV=true vite --force",
        "dev-custom": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW=true SW_DEV=true vite --force",
        "run-build-sw": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true vite build",
        "run-build-custom-sw": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true vite build",
        "start-sw": "npm run run-build-sw && npm run serve",
        "serve": "serve dist",
        "start-preview": "vite preview --port=4173",
        "test-custom-sw": "npm run run-build-custom-sw && SW=true vitest run && SW=true playwright test",
        "test-generate-sw": "npm run run-build-sw && vitest run && playwright test",
        "test": "npm run test-generate-sw && npm run test-custom-sw",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "types": "tsc --noEmit",
        "update-packages": "ncu --interactive --format group"
    },
    "dependencies": {
        "@mdi/js": "^7.4.47",
        "@mdi/react": "^1.6.1",
        "boardgame.io": "^0.50.2",
        "firebase": "^10.8.1",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "match-sorter": "^6.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-firebase-hooks": "^5.1.1",
        "react-router-dom": "^6.22.3",
        "sort-by": "^1.2.0",
        "tailwind-merge": "^2.2.1",
        "theme-change": "^2.5.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.10",
        "@types/lodash": "^4.17.0",
        "@types/react": "^18.2.65",
        "@types/react-dom": "^18.2.22",
        "@types/redux-logger": "^3.0.13",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.18",
        "daisyui": "^4.7.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "npm-check-updates": "^16.14.15",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "redux-logger": "^3.0.6",
        "rimraf": "^5.0.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.2",
        "vite": "^5.1.6",
        "vite-plugin-pwa": "^0.19.8",
        "vitest": "^1.3.1"
    }
}
